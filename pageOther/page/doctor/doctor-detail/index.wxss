.invite {
  /* display: none; */
  width:686rpx;
  height:164rpx;
  font-size: 25rpx;
  position:absolute;
  top:248rpx;
  left:32rpx;
  border: 1px solid #f0f0f0;
  border-radius: 5px;
  box-shadow: #f0f0f0 2px 2px 16px 3px;
}
.invite:after {
  content: '';
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  top: -10rpx;
  left:-2000rpx;
  transform: rotate(45deg);
  background-color: #fff;
  border: 1px #f0f0f0;
  border-style: solid none none solid ;
}
.arrow-one:after{
  left: 130rpx;
}
.arrow-two:after{
  left: 330rpx;
}
.arrow-three:after{
  left: 530rpx;
}


.specialty {
  font-size: 28rpx;
  color: #858585;
  width: 76%;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  display: -webkit-box !important;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  margin: 0;
  padding: 0;
  box-sizing: border-box !important;
  -webkit-box-sizing: border-box !important;
  word-wrap: break-word !important;
  outline: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}