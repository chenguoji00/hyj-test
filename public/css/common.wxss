page{
  --theme-color:#3d99fe;
  --bg-withe: #ffffff;
  --bg-gray: #f9f9f9;
  --textColor: #666;
  --grayTextColor: #858b9c;
  --grayTextColor2:#b1b1b1;
  --noActiveColor:#e2e4ea;
  --blackTextColor:#000;
  --bg-f5: #f5f5f5;
  --textWhiteColor:rgba(245, 239, 239, 1);
  --main-theme-color:rgba(21, 210, 195, 1);
}
.specialty-set {
  font-size: 28rpx;
  margin-top: 21.3333rpx;
  line-height: 35rpx;
  color: #666;
  overflow: hidden;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  display: -webkit-box !important;
  -webkit-box-orient: vertical;
  margin: 0;
  padding: 0;
  box-sizing: border-box !important;
  -webkit-box-sizing: border-box !important;
  word-wrap: break-word !important;
  outline: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
.one-line {
  -webkit-line-clamp: 1;
}
.two-line {
  -webkit-line-clamp: 2;
}

.three-line {
  -webkit-line-clamp: 3;
}